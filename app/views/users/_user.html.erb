<div id="<%= dom_id user %>">
  <p>
    <strong>Netid:</strong>
    <%= user.netid %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Role:</strong>
    <%= user.role %>
  </p>

  <p>
    <strong>Last login at:</strong>
    <%= user.last_login_at %>
  </p>

</div>
